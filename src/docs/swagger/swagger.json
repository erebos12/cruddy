                "summary": "delete all users",
                "responses": {
                    "200": {
                        "description": "successfully deleted all users",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/model.Message"
                        }
                    },
                    "404": {
                        "description": "could not delete all users",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/model.Message"
