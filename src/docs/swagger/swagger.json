                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "user successfully created",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/model.User"
                        }
                    },
                    "400": {
                        "description": "error reading message",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/model.Message"
                        }
                    }
                }
            },
            "delete": {
                "description": "delete all users which match attribute and value",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "users"
                ],
                "summary": "delete all users which match attribute and value",
                "parameters": [
                    {
                        "type": "string",
                        "description": "attribute of a certain json",
                        "name": "attribute",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "the value of the parameter attribute",
                        "name": "value",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successfully deleted",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/model.Message"
                        }
                    },
                    "404": {
                        "description": "could not delete user",
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/model.Message"
                        }
                    }
                }
            }
        },
        "/users/all": {
            "get": {
                "description": "get all user",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "users"
                ],
                "summary": "get all user",
